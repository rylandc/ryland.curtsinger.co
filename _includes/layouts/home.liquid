---
layout: layouts/base.liquid
siteTitle: Ryland Curtsinger
---
<header class="main-header">
    <a class="site-title" href="/">{{ siteTitle }}</a>
    <nav role="navigation" class="primary-nav">
        <ul>
        {% for page in collections.topLevel %}
            <li><a href="{{ page.data.page.url }}">{% if page.data.navName %}  {{ page.data.navName }} {% else %} {{ page.data.pageTitle }} {% endif %}</a></li>
        {% endfor %}
        </ul>
    </nav>
</header>
<main>
{{ content }}
</main>